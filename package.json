  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate --config=drizzle-dev.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle-dev.config.ts",
    "db:push": " drizzle-kit push --config=drizzle-dev.config.ts",
    "db:generate:prod": "NODE_ENV=production drizzle-kit generate --config=drizzle-prod.config.ts",
    "db:migrate:prod": " NODE_ENV=production  drizzle-kit migrate --config=drizzle-prod.config.ts",
    "db:push:prod": "NODE_ENV=production  drizzle-kit push --config=drizzle-prod.config.ts",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
